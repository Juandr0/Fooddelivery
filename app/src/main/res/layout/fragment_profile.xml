<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context="fragment.user.ProfileFragment">

<!-- TODO: Update blank fragment layout -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent">


        <TextView
            android:id="@+id/orderAgainTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="167dp"
            android:layout_marginTop="196dp"
            android:layout_marginEnd="153dp"
            android:text="@string/order_again"
            android:textColor="#000000"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/greetingsTextView" />

        <TextView
            android:id="@+id/lastOrderRestaurantNameTextView"
            android:layout_width="185dp"
            android:layout_height="20dp"
            android:layout_marginStart="70dp"
            android:layout_marginTop="20dp"
            android:text="@string/restaurant"
            android:textColor="#000000"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.185"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/orderAgainTextView" />

        <TextView
            android:id="@+id/lastOrderTextView"
            android:layout_width="185dp"
            android:layout_height="50dp"
            android:layout_marginStart="72dp"
            android:layout_marginTop="4dp"
            android:text="Grand Chicken Sandwich-mÃ¥l"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.175"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/lastOrderRestaurantNameTextView" />

        <Button
            android:id="@+id/button"
            android:layout_width="92dp"
            android:layout_height="48dp"
            android:layout_marginStart="35dp"
            android:layout_marginEnd="45dp"
            android:backgroundTint="@color/woopwoop_green"
            android:text="@string/order_again"
            android:textSize="8sp"
            app:layout_constraintBottom_toBottomOf="@+id/lastOrderRestaurantNameTextView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.355"
            app:layout_constraintStart_toEndOf="@+id/lastOrderTextView" />

        <Button
            android:id="@+id/button2"
            android:layout_width="106dp"
            android:layout_height="48dp"
            android:layout_marginStart="146dp"
            android:layout_marginTop="36dp"
            android:layout_marginEnd="147dp"
            android:background="#00FFFFFF"
            android:text="@string/order_history"
            android:textColor="#000000"
            android:textSize="8sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/lastOrderTextView" />

        <Button
            android:id="@+id/profileSignOutButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:backgroundTint="@color/black"
            android:text="Sign Out"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button2" />

        <TextView
            android:id="@+id/greetingsTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="80dp"
            android:layout_marginTop="36dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>